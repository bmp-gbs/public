=VLOOKUP(G7, [OtherWorkbook.xlsx]Sheet1!G:C, 2, FALSE)

=VLOOKUP(G7, '[otherworkbook.xlsx]Sheet1'!$A$1:$G$10000, 3, FALSE)

=VLOOKUP(G7,'[WorkbookName.xlsx]SheetName'!$A:$C,3,FALSE)

=VLOOKUP(G7,'[WorkbookName.xlsx]SheetName'!$A:$G,3,FALSE)

=INDEX('[otherworkbook.xlsx]SheetName'!$C:$C, MATCH("G7", '[otherworkbook.xlsx]SheetName'!$G:$G, 0))


=INDEX('[otherworkbook.xlsx]SheetName'!$H:$H, MATCH("CCI:" & G7, '[otherworkbook.xlsx]SheetName'!$G:$G, 0))
